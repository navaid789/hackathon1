{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/^(.*)$",
      "dest": "/index.html",
      "status": 200
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}